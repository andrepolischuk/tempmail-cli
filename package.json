  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrepolischuk/tempmail-cli/issues"
  },
  "homepage": "https://github.com/andrepolischuk/tempmail-cli#readme",
  "dependencies": {
    "chalk": "^1.1.1",
    "configstore": "^2.0.0",
    "meow": "^3.7.0",
    "read-pkg": "^1.1.0",
    "tempmail-wrapper": "github:xomyaq/tempmail"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
